{
  "name": "TEF Synthetic Data Pipeline",
  "pipeline_id": "tef-synthetic-pipeline",
  "creation_date": "2025-01-08",
  "type": "pipeline-topology/v2",
  "version": "2.0",
  "nodes": [
    {
      "container_name": "data_loader",
      "proto_uri": "data_provision.proto",
      "image": "tef-data-provision-adapter:latest",
      "node_type": "DataSource",
      "operation_signature_list": [
        {
          "operation_signature": {
            "operation_name": "LoadData",
            "output_message_name": "LoadDataResponse"
          },
          "connected_to": [
            {
              "container_name": "feature_engineer",
              "operation_signature": {
                "operation_name": "ApplyFeatures"
              }
            }
          ]
        }
      ]
    },
    {
      "container_name": "feature_engineer",
      "proto_uri": "knowledge_store.proto",
      "image": "tef-knowledge-store-adapter:latest",
      "node_type": "MLModel",
      "operation_signature_list": [
        {
          "operation_signature": {
            "operation_name": "ApplyFeatures",
            "input_message_name": "ApplyFeaturesRequest",
            "output_message_name": "ApplyFeaturesResponse"
          },
          "connected_to": [
            {
              "container_name": "model_trainer",
              "operation_signature": {
                "operation_name": "TrainModel"
              }
            }
          ]
        }
      ]
    },
    {
      "container_name": "model_trainer",
      "proto_uri": "synthetic_data.proto",
      "image": "tef-synthetic-data-adapter:latest",
      "node_type": "MLModel",
      "operation_signature_list": [
        {
          "operation_signature": {
            "operation_name": "TrainModel",
            "input_message_name": "TrainModelRequest",
            "output_message_name": "TrainModelResponse"
          },
          "connected_to": [
            {
              "container_name": "data_generator",
              "operation_signature": {
                "operation_name": "GenerateData"
              }
            }
          ]
        }
      ]
    },
    {
      "container_name": "data_generator",
      "proto_uri": "synthetic_data.proto",
      "image": "tef-synthetic-data-adapter:latest",
      "node_type": "DataSink",
      "operation_signature_list": [
        {
          "operation_signature": {
            "operation_name": "GenerateData",
            "input_message_name": "GenerateDataRequest",
            "output_message_name": "GenerateDataResponse"
          },
          "connected_to": []
        }
      ]
    }
  ]
}
